AutoTab:CreateToggle("BEST PETS FREE", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.a = true 
        while wait(0) and _G.a do
     local args = {
    [1] = "Equip",
    [2] = {
        [3] = "Broken Egg4",
